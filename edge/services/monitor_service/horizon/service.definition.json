{
    "org": "$HZN_ORG_ID",
    "label": "Monitor Service for $ARCH",
    "description": "Polls the motion sensor for movement and then gets a picture from the camera sensor.",
    "public": true,
    "documentation": "",
    "url": "$MONITOR_NAME",
    "version": "$MONITOR_VERSION",
    "arch": "$ARCH",
    "sharable": "singleton",
    "matchHardware": null,
    "requiredServices": [
        {
            "url": "$CAMERA_NAME",
            "org": "$HZN_ORG_ID",
            "version": "$CAMERA_VERSION",
            "arch": "$ARCH"
        },
        {
            "url": "$MOTION_NAME",
            "org": "$HZN_ORG_ID",
            "version": "$MOTION_VERSION",
            "arch": "$ARCH"
        }
    ],
    "userInput": [],
    "deployment": {
        "services": {
            "monitor": {
                "image": "$DOCKER_HUB_ID/$MONITOR_NAME:$MONITOR_VERSION",
                "privileged": true
            }
        }
    },
    "deploymentSignature": "",
    "imageStore": null
}
